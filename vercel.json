{
  "installCommand": "cd ./frontend & npm install",
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "/api/{index,web,sydney,sys-config}.go",
      "use": "@vercel/go"
    }
  ],
  "rewrites": [
    {
      "source": "/sysconf",
      "destination": "/api/sys-config.go"
    },
    {
      "source": "/sydney/.*",
      "destination": "/api/sydney.go"
    },
    {
      "source": "/web/.*",
      "destination": "/api/web.go"
    },
    {
      "source": "/.*",
      "destination": "/api/index.go"
    }
  ]
}